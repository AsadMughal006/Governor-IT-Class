
let num1=5;
let num2=3;
let add=num1+num2;
console.log("Addition = " + add);


let num3=10;
let num4=2;
let sub=num3-num4;
console.log("Subtraction = " + sub);


let num5=4;
let num6=2;
let multi=num5*num6;
console.log("Multiplication = " + multi);


let num7=16;
let num8=2;
let div=num7/num8;
console.log("Division = " + div);

